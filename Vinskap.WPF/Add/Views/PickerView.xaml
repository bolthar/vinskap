<UserControl x:Class="Vinskap.WPF.Add.Views.PickerView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ComboBox Width="200" IsEditable="True" IsTextSearchEnabled="True" ItemsSource="{Binding Items}" SelectedItem="{Binding SelectedItem, ValidatesOnDataErrors=True}" Validation.ErrorTemplate="{StaticResource ResourceKey=DefaultErrorTemplate}"></ComboBox>
</UserControl>
